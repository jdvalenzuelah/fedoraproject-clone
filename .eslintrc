{
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/recommended"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
          "pragma": "React",
          "version": "^16.9.0",
        },
    },
    "rules": {
        "no-console": 0,
    },
    "plugins": [
      "import",
      "react"
    ]
   }